{
  "name": "bkmexpress",
  "version": "2.1.0",
  "description": "Nodejs api for bkm express payment system (Turkey)",
  "main": "./lib/bkmexpress.js",
  "typings": "./lib/bkmexpress.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "lint": "tslint src/{,**/}*.ts --config tslint.json --project tsconfig.json --fix",
    "tsc": "tsc",
    "build": "node_modules/.bin/rimraf lib && npm run tsc",
    "pretest": "npm run build",
    "test": "make test",
    "preversion": "node_modules/.bin/rimraf src/**/*.js && npm run lint && npm test",
    "version": "git add -A lib",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brendtumi/bkmexpress.git"
  },
  "keywords": [
    "BKM",
    "bkm express",
    "turkey",
    "payment system",
    "payment integration",
    "bex2",
    "bkm-exp"
  ],
  "author": "Tümay Çeber <tumayceber@gmail.com> (https://about.me/tumayceber)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brendtumi/bkmexpress/issues"
  },
  "homepage": "https://github.com/brendtumi/bkmexpress",
  "dependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/debug": "0.0.30",
    "@types/lodash": "^4.14.104",
    "@types/node": "^9.4.6",
    "@types/request": "^2.47.0",
    "@types/request-promise": "^4.1.41",
    "@types/semver": "^5.5.0",
    "@types/source-map-support": "^0.4.0",
    "bluebird": "^3.5.1",
    "debug": "^3.1.0",
    "lodash": "^4.17.5",
    "node-rsa": "^0.4.2",
    "request": "2.84.0",
    "request-promise": "^4.2.2",
    "semver": "^5.5.0",
    "source-map-support": "^0.5.3"
  },
  "devDependencies": {
    "@types/chai": "3.5.x",
    "@types/mocha": "^2.2.48",
    "chai": "3.5.x",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": ">0.10.0"
  }
}
